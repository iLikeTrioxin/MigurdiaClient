{
  "name": "Migurdia",
  "description": "Migurdia is client for database full of images sorted by tags.",
  "keywords": ["migurdia", "image", "explorer"],
  "homepage": "https://github.com/iLikeTrioxin/MigurdiaClient#readme",
  "bugs": "https://github.com/iLikeTrioxin/MigurdiaClient/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/iLikeTrioxin/MigurdiaClient.git"
  },
  "version": "0.6.15",
  "author": {
    "name": "iLikeTrioxin",
    "email": "iLike135Trioxane@protonmail.com",
    "url": "https://yukiteru.xyz/"
  },
  "license": "AGPL-3.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "publishApp": "electron-builder --linux --win -p always"
  },
  "dependencies": {
    "@yaireo/tagify": "^4.9.2",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.6.1",
    "jimp": "^0.16.1",
    "masonry-layout": "^4.2.2",
    "node-fetch": "^2.6.1",
    "plupload": "^2.3.7",
    "prop-types": "^15.7.2",
    "sjcl": "^1.0.8",
    "tslib": "^2.3.1",
    "video-snapshot": "^1.0.11"
  },
  "devDependencies": {
    "electron": "^15.1.1",
    "electron-builder": "^22.14.5"
  },
  "build": {
    "appId": "com.github.iLikeTrioxin.MigurdiaClient",
    "publish": [
      {
        "provider": "github",
        "owner": "iLikeTrioxin",
        "repo": "MigurdiaClient"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "category": "Graphics",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "pacman",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  }
}
